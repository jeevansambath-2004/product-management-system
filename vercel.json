{
  "builds": [
    {
      "src": "Frontend/product-management-system/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Frontend/product-management-system/dist/index.html"
    }
  ] ,
    "env": {
        "API_URL": "https://product-management-system345.onrender.com"
    }

}
